{% extends 'base.html.twig' %}
{% block body %}
       <div class="container-fluid" id="indo">
        <div class="row">
          <div class="col-lg-12 col-md-12 col-sm-12">
<h2 class="page-header"> Events you can visit in Vienna </h2>
</div>
</div>
<div class="row">
          <div class="col-lg-12 col-md-12 col-sm-12">
  <table class="table table-striped" id="tab">
   <thead>
       <tr>
           <th>Id</th>
           <th>NAME</th>
           <th>DATE</th>
           <th>Description</th>
           <th>Image</th>
           
           <th></th>
        </tr>
   </thead>
   {% for test1 in tests %}
   <tbody>
       <tr>
          <th scope="row">{{test1.id}}</th>
           <td>{{test1.name}}</td>
           <td>{{test1.date|date('F j, Y, g:i a')}}</td>
           <td>{{test1.description}}</td>
           <td>{{test1.image}}</td>
           
           <td> <a href="/test1/details/{{test1.id}}" class="btn btn-success">View</a>
                   
            <a href="/test1/edit/{{test1.id}}" class="btn btn-primary">Edit</a>
           <a href="/test1/delete/{{test1.id}}" class="btn btn-danger">Delete</a>      
           </td>
       </tr> 
       {% endfor %}
   </tbody>
   </table>
 </div>
</div>
</div>


{% endblock %}
